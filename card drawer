      $('.template-product #add-to-cart-form').submit(function( event ) {
        event.preventDefault();
   // alert('this');
        $.ajax({
          url: '/cart/add.js',
          dataType: 'json',
          type: 'post',
          data: $(this).serialize(),
          success: function(res) {
            updateDrawerCart();
            //window.location.href = "/cart";
        	},
          error: function(XMLHttpRequest, textStatus) {
          console.log("Error");
          }
        });

      });


      $("[id^=product-form-]").submit(function( event ) {

        event.preventDefault();

        $.ajax({
          url: '/cart/add.js',
          dataType: 'json',
          type: 'post',
          data: $(this).serialize(),
          success: function(res) {
            updateDrawerCart();
            //window.location.href = "/cart";
        	},
          error: function(XMLHttpRequest, textStatus) {
          console.log("Error");
          }
        });

      });


      $('body').on('click', '#new-drawer-item-container .drawer_cart_item_remove', function() {
        var quantity =  0;
        var lineID = $(this).data('lineid');
        $.ajax({
          type: 'POST',
          url: '/cart/change.js',
          data: 'quantity=' + quantity + '&id=' + lineID,
          dataType: 'json',
          success: function(line_item) {
            updateDrawerCart();
          },
          error: function(XMLHttpRequest, textStatus) {
            console.log("Error");
          }
        });
      });

      $('body').on('click', '#new-drawer-item-container .qty-plus', function() {
        var currentqty = $(this).closest('.qty-inner').find('.qty-input').val();
        var lineId = $(this).data('lineid');
        currentqty = currentqty * 1;
        currentqty = currentqty + 1;
        $(this).closest('.qty-inner').find('.qty-input').val(currentqty);
        $.ajax({
          type: 'POST',
          url: '/cart/change.js',
          data: 'quantity=' + currentqty + '&id=' + lineId,
          dataType: 'json',
          success: function(line_item) {
            updateDrawerCart();
          },
          error: function(XMLHttpRequest, textStatus) {
            console.log("Error");
          }
        });
      });

      $('body').on('click', '.upsell-slider .upsell-add-to-cart', function() {

        dt = {
          "quantity": 1,
          "id": $(this).data("vid")
        }

        $.ajax({
          url: '/cart/add.js',
          dataType: 'json',
          type: 'post',
          data: dt,
          success: function(res) {
            updateDrawerCart();
            //window.location.href = "/cart";
        	},
          error: function(XMLHttpRequest, textStatus) {
          console.log("Error");
          }
        });
      });

      $('body').on('click', '#new-drawer-item-container .qty-minus', function() {
        var currentqty = $(this).closest('.qty-inner').find('.qty-input').val();
        var lineId = $(this).data('lineid');
        currentqty = currentqty * 1;
        if(currentqty > 0){
          currentqty = currentqty - 1;
          $(this).closest('.qty-inner').find('.qty-input').val(currentqty);

          if(currentqty>0){
            $.ajax({
              type: 'POST',
              url: '/cart/change.js',
              data: 'quantity=' + currentqty + '&id=' + lineId,
              dataType: 'json',
              success: function(line_item) {
                updateDrawerCart();
              },
              error: function(XMLHttpRequest, textStatus) {
                console.log("Error");
              }
            });
          }else{
            var quantity = 0;
            $.ajax({
            type: 'POST',
            url: '/cart/change.js',
            data: 'quantity=' + quantity + '&id=' + lineId,
            dataType: 'json',
            success: function(line_item) {
              updateDrawerCart();
            },
            error: function(XMLHttpRequest, textStatus) {
              console.log("Error");
            }
          });
          }

        }
      });

      jQuery( ".slide-menu-cart-new-drawer" ).on( "click", function() {
        jQuery('#new-drawer-cart').toggleClass('new-drawer-open');
        jQuery('#new-drawer-overlay').toggleClass('new-drawer-open');
      });

      function updateDrawerCart(){

        $.ajax({
          url: "../../../cart.js",
          type : 'GET',
          dataType:'json',
          success: function(result){

            var itemFinalStr = "" ;
            var upselStr = "";
            var cartItemStr = "";
        let trand_id = ""; 
            let find_discount ="";
            let quantity ="";
            for(var i=0; i< result.items.length ; i++ ){
 trand_id += result.items[i].id+" ";
        quantity += result.items[i].quantity+" ";
     let find_discount ="";
 $.getJSON(window.Shopify.routes.root + 'products/'+result.items[i].handle+'.js', function(product) {	
