            <script>
                       // Event listener voor wanneer er producten worden toegevoegd of verwijderd uit de winkelwagen
              document.addEventListener("cartUpdate", function() {
                let originalPrices = 0;
                let comparePrices = 0;

                // Loop door alle producten in de winkelwagen
                for (let i = 0; i < cart.items.length; i++) {
                  originalPrices += cart.items[i].originalPrice;
                  comparePrices += cart.items[i].comparePrice;
                }

                // Bereken het verschil tussen original en compare prijzen
                let discount = originalPrices - comparePrices;

                // Toon de discount prijs aan de gebruiker
                document.getElementById("discount-price").innerHTML = discount;
              });
            </script>

